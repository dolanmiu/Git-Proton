{
  "name": "git-proton",
  "version": "0.2.0",
  "description": "An open source Electron based Git GUI tool",
  "scripts": {
    "start-web": "webpack-dev-server --open --inline --progress --port 8080",
    "start": "npm run build && electron .",
    "build-main": "webpack --config config/webpack.main.js --progress --profile --bail --display-error-details",
    "build-electron": "webpack --config config/webpack.prod.js --progress --profile --bail --display-error-details",
    "build": "rimraf dist && npm run build-electron && npm run build-main",
    "package": "npm run build && npm run prepare && ./node_modules/electron-packager/cli.js dist App --platform=darwin --arch=all --out=dist-app --overwrite",
    "electron": "electron .",
    "karma": "karma start 'config/karma.conf.js'",
    "lint": "tslint --project .",
    "test": "npm run karma && npm run lint"
  },
  "licenses": [
    {
      "type": "GPL-3.0",
      "url": "https://github.com/dolanmiu/Git-Proton/blob/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dolanmiu/Git-Proton.git"
  },
  "license": "gpl-3.0",
  "main": "./dist/main.js",
  "dependencies": {
    "@angular/animations": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/router": "^4.1.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "material-design-icons": "^3.0.1",
    "mkdirp": "^0.5.1",
    "nconf": "^0.8.4",
    "nodegit": "^0.18.3",
    "octicons": "^5.0.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/electron": "^1.4.26",
    "@types/jasmine": "^2.5.41",
    "@types/karma": "^0.13.33",
    "@types/mkdirp": "^0.3.29",
    "@types/nconf": "0.0.32",
    "@types/node": "^7.0.14",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^3.1.2",
    "codelyzer": "^3.0.0",
    "css-loader": "^0.23.1",
    "electron": "^1.6.6",
    "electron-packager": "^8.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.10.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
